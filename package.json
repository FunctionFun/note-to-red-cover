{
	"name": "note-to-red-cover",
	"version": "1.0.0",
	"description": "将Obsidian笔记导出为小红书封面图片",
	"main": "main.ts",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"package": "mkdir -p release && cp dist/main.js dist/styles.css manifest.json release/ && zip -r release/note-to-red-cover.zip release/*"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"小红书封面图"
	],
	"author": "FunctionFun",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"eslint-plugin-unused-imports": "^4.3.0",
		"fs-extra": "^11.3.2",
		"html-to-image": "^1.11.13",
		"html2canvas": "^1.4.1",
		"jszip": "^3.10.1",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@types/prismjs": "^1.26.5",
		"prismjs": "^1.30.0"
	}
}
